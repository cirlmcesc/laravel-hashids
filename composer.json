{
    "name": "cirlmcesc/laravel-hashids",
    "description": "Automatically generate short, unique, non-sequential ids (like YouTube and Bitly) from numbers for the Eloquent model.",
    "license": "MIT",
    "type": "laravel-package",
    "keywords": [
        "laravel",
        "model",
        "decode",
        "encode",
        "hash",
        "hashids",
        "hashid"
    ],
    "authors": [
        {
            "name": "Cirlmcesc Ma",
            "email": "cirlmcesc_ma@163.com"
        }
    ],
    "homepage": "https://github.com/cirlmcesc/laravel-hashids",
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "hashids/hashids": "^5.0",
        "illuminate/support": "^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Cirlmcesc\\LaravelHashids\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Cirlmcesc\\LaravelHashids\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cirlmcesc\\LaravelHashids\\Providers\\LaravelHashidsServiceProvider"
            ]
        }
    }
}