{
    "name": "cirlmcesc/laravel-hashids",
    "description": "Automatically hash the field with ID in the Eloquent model.",
    "license": "MIT",
    "type": "laravel-package",
    "keywords": [
        "laravel",
        "hashids",
        "model"
    ],
    "authors": [
        {
            "name": "Cirlmcesc Ma",
            "email": "cirlmcesc_ma@163.com"
        }
    ],
    "homepage": "https://github.com/cirlmcesc/laravel-hashids",
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0.2",
        "hashids/hashids": "^4.1.0",
        "illuminate/support": "^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.4"
    },
    "autoload": {
        "psr-4": {
            "Cirlmcesc\\LaravelHashids\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Cirlmcesc\\LaravelHashids\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cirlmcesc\\LaravelHashids\\Providers\\LaravelHashidsServiceProvider"
            ]
        }
    }
}